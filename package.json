{
  "name": "finance-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "api-watch": "node_modules/.bin/nodemon graphql-server.js --ignore gen",
    "build": "node_modules/.bin/vue-cli-service build",
    "lint": "node_modules/.bin/vue-cli-service lint"
  },
  "dependencies": {
    "apollo-server": "^3.10.2",
    "axios": "^0.26.1",
    "core-js": "^3.3.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "danfojs": "^1.1.0",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "graphql-server": "^0.3.2",
    "knex": "^2.2.0",
    "multer": "^1.4.5-lts.1",
    "objection": "^2.1.3",
    "objection-graphql": "^0.4.5",
    "pg": "^8.0.3"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "nodemon": "^1.19.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
